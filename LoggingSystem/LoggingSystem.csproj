    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <UserSecretsId>4562a579-4177-44ff-8c17-cc17e5621809</UserSecretsId>
